<div id="rel-{{ relation.id }}" class="union">
	<div class="marriage">
		<div class="logo">
			{% if relation.type == 0 %}
				<img width="15px" src="{{ asset("img/icons/rings.png") }}" alt="Mariage">
			{% elseif relation.type == 1 %}
				<img width="15px" src="{{ asset("img/icons/pacs.png") }}" alt="pacs">
			{% elseif relation.type == 2 %}
				<img width="15px" src="{{ asset("img/icons/heart.png") }}" alt="Union Libre">
			{% else %}
				<img width="15px" alt=" ">
			{% endif %}
		</div>
		<div class="date">
			{% if relation.start %}
				{{ relation.start | localizeddate('medium', 'none') }}
			{% else %}
				<br>
			{% endif %}
		</div>
	</div>
</div>